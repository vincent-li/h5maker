  "private": true,
  "repository": "https://github.com/brandonxiang/svelte-webpack-template.git",
  "author": "brandonxiang <1542453460@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.23.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "file-loader": "^6.2.0",
    "globby": "^11.0.3",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.4",
    "postcss": "^8.2.9",
    "postcss-load-config": "^3.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.31.2",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.7.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.16.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack serve --content-base public",
    "analyze": "NODE_ENV=production webpack --env.analyze=true --mode=production --progress",
    "format": "prettier --write --svelte-sort-order scripts-markup-styles ./**/*.svelte"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "dexie": "^3.2.1",
    "mini-html-webpack-plugin": "^3.1.3",
    "moment": "^2.29.2",
    "pdf": "^0.1.0",
    "svelte": "^3.31.2",
    "svelte-inline-svg": "^1.1.2",
    "svelte-material-ui": "^5.0.0-beta.5",
    "webpack-plugin-serve": "^1.6.0"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.svelte": [
      "prettier --write --svelte-sort-order scripts-markup-styles ./**/*.svelte",
      "git add"
    ],
    "**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
